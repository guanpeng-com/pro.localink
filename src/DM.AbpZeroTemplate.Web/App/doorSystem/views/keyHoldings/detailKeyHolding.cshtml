<div>
    <div class="modal-header">
        <h4 class="modal-title">
            <span>@L("KeyHoldingDetail")</span>
        </h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal audit-log-detail-view" role="form">
            <div class="form-body">
                <h3 class="form-section">@L("KeyHoldingDetail")</h3>
                <div class="row">
                    <div class="col-md-12">

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("VisitorName"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.visitorName}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("Password"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.password}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("VisiteStartTime"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.visiteStartTime}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("VisiteEndTime"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.visiteEndTime}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("IsCollection"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.isCollection}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("CollectionTime"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.collectionTime}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("HomeOwerName"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.homeOwerName}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-4">@L("CommunityName"):</label>
                            <div class="col-sm-8">
                                <p class="form-control-static">
                                    {{vm.keyHolding.communityName}}
                                </p>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" ng-click="vm.close()">@L("Close")</button>
    </div>
</div>



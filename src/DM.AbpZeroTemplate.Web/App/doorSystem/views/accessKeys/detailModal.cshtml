<div>
    <div class="modal-header">
        <h4 class="modal-title">
            <span>@L("AccessKeyDetail")</span>
        </h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal audit-log-detail-view" role="form">
            <div class="form-body">
                <h3 class="form-section">@L("AccessKeyDetail")</h3>
                <div class="row">
                    <div class="col-md-12">
			
                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("AccessKeyDoorName"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.accessKey.doorName}}
                                </p>
                            </div>
                        </div>
			
                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("AccessKeyHomeOwerName"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.accessKey.homeOwerName}}
                                </p>
                            </div>
                        </div>
			
                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("AccessKeyValidity"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.accessKey.validity}}
                                </p>
                            </div>
                        </div>
			
                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("AccessKeyLockId"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.accessKey.lockId}}
                                </p>
                            </div>
                        </div>
			
                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("AccessKeyIsAuth"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.accessKey.isAuth}}
                                </p>
                            </div>
                        </div>
																					
                    </div>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" ng-click="vm.close()">@L("Close")</button>
    </div>
</div>



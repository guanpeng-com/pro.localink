                </div>
                <div class="portlet-body">
                    <div ng-if="vm.accessKeys.gridOptions.data" ui-grid="vm.accessKeys.gridOptions" ui-grid-pagination ui-grid-resize-columns dir=@(CultureHelper.IsRtl ? "rtl" : "")></div>
                    <div ng-if="!vm.accessKeys.gridOptions.data" class="text-muted">
                        @L("NoTenantSelected")
                    </div>
                </div>
            </div>
        </div>
    </div>

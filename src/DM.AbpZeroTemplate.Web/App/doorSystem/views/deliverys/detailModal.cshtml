<div>
    <div class="modal-header">
        <h4 class="modal-title">
            <span>@L("DeliveryDetail")</span>
        </h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal audit-log-detail-view" role="form">
            <div class="form-body">
                <h3 class="form-section">@L("DeliveryDetail")</h3>
                <div class="row">
                    <div class="col-md-12">

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("DeliveryId"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.delivery.id}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("DeliveryTitle"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.delivery.title}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("HomeOwerName"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.delivery.homeOwerName}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("DeliveryIsGather"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.delivery.isGather}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group" ng-if="vm.delivery.isGather">
                            <label class="control-label col-sm-3">@L("DeliveryGatherTime"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.delivery.gatherTime}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group" ng-if="vm.delivery.isReplace">
                            <label class="control-label col-sm-3">@L("DeliveryIsReplace"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.delivery.isReplace}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group" ng-if="vm.delivery.isReplace">
                            <label class="control-label col-sm-3">@L("DeliveryReplaceHomeOwerName"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.delivery.replaceHomeOwerName}}
                                </p>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" ng-click="vm.close()">@L("Close")</button>
    </div>
</div>



<div>
    <div class="modal-header">
        <h4 class="modal-title">
            <span>@L("DoorDetail")</span>
        </h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal audit-log-detail-view" role="form">
            <div class="form-body">
                <h3 class="form-section">@L("DoorDetail")</h3>
                <div class="row">
                    <div class="col-md-12">

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("DoorName"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.door.name}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("DoorPId"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.door.pId}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("DoorDepartId"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.door.departId}}
                                </p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-sm-3">@L("DoorIsAuth"):</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.door.isAuth}}
                                </p>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" ng-click="vm.close()">@L("Close")</button>
    </div>
</div>

